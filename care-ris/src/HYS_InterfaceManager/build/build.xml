    <copy file="${cc.dir}\SRC\Framework\DropDB.sql" todir="${build.dir}\HYSInterfaceManager\db_install" overwrite="true"/>

    <exec executable="cmd.exe" dir="${cc.dir}" failonerror="Yes">
      <arg line="/c attrib -R *.* /S /D"/>
    </exec>

    <antcall target="build_HYSInterfaceManager"/>
  </target>



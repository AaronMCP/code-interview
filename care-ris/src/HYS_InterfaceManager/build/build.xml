  <target name="build_main">

    <echo message="--- Make directory ---"/>
    <delete dir="${build.dir}"/>
    <mkdir dir="${build.dir}"/>

    <echo message="--- Copy dicom dll ---"/>
    <copy todir="${build.dir}\HYSInterfaceManager\foDicom_lib" overwrite="true">
      <fileset dir="${cc.dir}\3rdParty\foDicom"/>
    </copy>

    <echo message="--- Copy osql.exe for x86 ---" />
    <copy todir="${build.dir}\HYSInterfaceManager\osql" overwrite="true">
      <fileset dir="${cc.dir}\3rdParty\osql\x86" />
    </copy>


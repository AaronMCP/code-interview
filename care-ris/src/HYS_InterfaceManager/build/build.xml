    <copy file="${cc.dir}\3rdParty\Chinese2English\JKTCPYCnvt_BIG5.dll" todir="${build.dir}\HYSInterfaceManager\pinyin_lib" overwrite="true"/>
    <copy file="${cc.dir}\3rdParty\Chinese2English\JKTCPYCnvt_GB.dll" todir="${build.dir}\HYSInterfaceManager\pinyin_lib" overwrite="true"/>

    <echo message="--- Copy database script ---"/>
